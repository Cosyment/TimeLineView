<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TimeLineView">
        <attr name="titleTextSize" format="dimension"/>
        <attr name="titleTextColor" format="color"/>
        <attr name="dateTextSize" format="dimension"/>
        <attr name="dateTextColor" format="color"/>
        <attr name="circleRadius" format="dimension"/>
        <attr name="circleColor" format="color"/>
        <attr name="currentCircleColor" format="color"/>
        <attr name="lineColor" format="color"/>
        <attr name="currentDrawable" format="reference"/>
        <attr name="drawable" format="reference"/>
        <attr name="lastDrawable" format="reference"/>
        <attr name="errorDrawable" format="reference"/>
        <attr name="pointGravity" format="enum">
            <enum name="top" value="0"/>
            <enum name="center" value="1"/>
            <enum name="bottom" value="2"/>
        </attr>
    </declare-styleable>
</resources>